function submitForms() {
	if (isUID() && isPassword() ) { return true }
	else {return false}
	};


function isUID() {
if (document.theForm.txtUID.value == "") {
alert ("\n The User Name field is blank. \n\n Please enter your user name.")
document.theForm.txtUID.focus();
return false;
}
return true;
}

function isPassword() {
if (document.theForm.txtPassword.value == "") {
alert ("\n The Password field is blank. \n\n Please enter your password.")
document.theForm.txtPassword.focus();
return false;
}
return true;
}

